{
    "name": "envatic/crud-strap",
    "license": "MIT",
    "description": "Laravel CRUD Generator",
    "keywords": [
        "laravel",
        "crud",
        "crud generator",
        "laravel crud generator",
        "api generator"
    ],
    "authors": [
        {
            "name": "Sohel Amin",
            "email": "sohelamincse@gmail.com",
            "homepage": "http://www.envatic.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0",
        "laravelcollective/html": "^5.3|^6.0"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~8.5|^9.5.10",
        "orchestra/testbench": "^3.3|^4.0|^5.0|^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Envatic\\CrudStrap\\": "src/"
        },
        "classmap": [
            "tests"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Envatic\\CrudStrap\\CrudStrapServiceProvider"
            ]
        }
    }
}